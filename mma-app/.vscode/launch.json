    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Remote",
            "address": "TCP/IP address of process to be debugged",
            "port": 3000,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "Absolute path to the remote directory containing the program",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Python: UFC Image Scraper",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\backend\\tools\\data tools\\scraper scripts\\ImageScraper.py",
            "console": "integratedTerminal",
            "cwd":"${workspaceFolder}\\backend\\tools\\data tools\\scraper scripts"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "JS: Database Creation",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}\\backend\\tools\\data tools\\data creation scripts\\dataCreationTool.js",
            "cwd":"${workspaceFolder}\\backend\\tools\\data tools\\data creation scripts\\"
        },
        {
            "name": "Python: Json Converter",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\backend\\tools\\data tools\\conversion scripts\\eventDataFilesToJson.py",
            "console": "integratedTerminal",
            "cwd":"${workspaceFolder}\\backend\\tools\\data tools\\conversion scripts"
        },
        {
            "name": "Python: UFC Event Scraper",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\backend\\tools\\data tools\\scraper scripts\\UfcEventScraper.py",
            "console": "integratedTerminal",
            "cwd":"${workspaceFolder}\\backend\\tools\\data tools\\scraper scripts"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "JS: Mock Database Creation",
            "program": "${workspaceFolder}\\backend\\tools\\data tools\\mockDataCreationTool.js"
        }
    ]
}